{
  "name": "@ebi-gene-expression-group/organ-anatomogram",
  "version": "1.0.0-beta1",
  "description": "Organ anatomogram component for Expression Atlas heatmap",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "prepack": "rm -rf lib && babel src -d lib --copy-files",
    "parseSvgs": "node scripts/svgParse.js src/svg > src/json/svgsMetadata.json",
    "dist": " webpack -p",
    "pretest": "npm run parseSvgs",
    "test": "jest --coverage",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme"
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "contributors": [
    "Wojciech Bazant <wbazant@ebi.ac.uk>",
    "Andrew Diamantoukos <andrewd@jimmy.harvard.edu>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/anatomogram.git"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-svg": "^7.2.7",
    "recompose": "^0.30.0",
    "styled-components": "^5.2.0",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@ebi-gene-expression-group/eslint-config": "^2.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "fast-xml-parser": "^3.17.4",
    "file-loader": "^6.1.1",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.5.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "react-test-renderer": "^16.14.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
