{
  "name": "scxa-gene-search-form",
  "version": "0.2.0",
  "description": "Single Cell Expression Atlas homepage gene search form",
  "main": "lib/index.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepack": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest --coverage --verbose",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <atlas-developers@ebi.ac.uk>",
  "collaborators": [
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/my-package.git"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-select": "^2.0.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "ajv": "^6.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^6.5.2",
    "jest": "^23.4.1",
    "react-test-renderer": "^16.4.1",
    "webpack": "^4.16.2",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  }
}
