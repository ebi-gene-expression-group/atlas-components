{
  "name": "@ebi-gene-expression-group/atlas-scientific-notation-number",
  "version": "1.0.4",
  "publishConfig": {
    "access": "public"
  },
  "description": "Format numbers nicely",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "build": "webpack-dev-server --mode=development"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-gene-expression-group/atlas-number-format.git"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.26.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@ebi-gene-expression-group/eslint-config": "^2.4.0",
    "babel-loader": "^8.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "jest": "^29.7.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^4.2.0",
    "react-test-renderer": "^16.13.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^ 6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "overrides": {
    "braces": "^3.0.3"
  }
}
