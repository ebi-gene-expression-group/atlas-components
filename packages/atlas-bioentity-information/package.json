{
  "name": "sc-atlas-bioentity-information",
  "version": "1.7.2",
  "description": "Bioentity information",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest --coverage --verbose",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__test__/Utils.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Monica Jianu <monica@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebi-gene-expression-group/atlas-bioentity-information.git"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "expression-atlas-autocomplete": "^3.0.1",
    "fetch-mock": "^7.2.5",
    "file-loader": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-fetch": "^2.2.1",
    "react-test-renderer": "^16.6.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
