{
  "name": "@ebi-gene-expression-group/atlas-bioentity-information",
  "version": "2.1.0",
  "description": "Bioentity information",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Monica Jianu <monica@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-gene-expression-group/atlas-components.git"
  },
  "dependencies": {
    "@ebi-gene-expression-group/atlas-react-fetch-loader": "^3.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "jest": "^26.4.0",
    "jest-enzyme": "^7.1.2",
    "random-string": "^0.2.0",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
