{
  "name": "sc-atlas-bioentity-information",
  "version": "1.2.1",
  "description": "Bioentity information",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server -d",
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepublishOnly": "rm -rf lib && babel src -d lib --copy-files",
    "testOnly": "jest --coverage --verbose",
    "test": "npm run testOnly",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Monica Jianu <monica@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/atlas-bioentity-information.git"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "expression-atlas-autocomplete": "^1.1.1",
    "fetch-mock": "^6.1.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.20.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
