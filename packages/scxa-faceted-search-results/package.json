{
  "name": "@ebi-gene-expression-group/scxa-faceted-search-results",
  "version": "4.2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Faceted experiments list for Single Cell Expression Atlas gene search results",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__test__/TestUtils.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebi-gene-expression-group/atlas-components.git"
  },
  "dependencies": {
    "@ebi-gene-expression-group/atlas-react-fetch-loader": "^3.10.0",
    "format-number": "^3.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^5.6.0",
    "react-tooltip": "^4.5.0",
    "styled-components": "^5.3.6",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.0",
    "@ebi-gene-expression-group/react-ebi-species": "^2.7.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.1",
    "cypress": "^11.0.1",
    "eslint": "^8.27.0",
    "eslint-config-gene-expression": "^0.5.0",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "node-fetch": "^3.2.10",
    "random-words": "^1.2.0",
    "react-test-renderer": "^17.0.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "gitHead": "0fcac2cf1b51445c92c92a4dc4ce51f468a1a6fb"
}
