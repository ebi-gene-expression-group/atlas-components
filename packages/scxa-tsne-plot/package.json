{
  "name": "expression-atlas-experiment-page-tsne-plot",
  "version": "1.13.3",
  "description": "t-SNE plot view tab for Single Cell Expression Atlas experiment page",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server -d",
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepublishOnly": "rm -rf lib && babel src -d lib --copy-files",
    "testOnly": "jest --coverage --verbose",
    "test": "yarn run testOnly",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Monica Jianu <monica@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/scxa-experiment-page-tsne-plot-view"
  },
  "dependencies": {
    "color": "^2.0.0",
    "deepmerge": "^2.0.0",
    "expression-atlas-autocomplete": "^1.2.0",
    "expression-atlas-number-format": "^2.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-highcharts": "^13.0.0",
    "seedrandom": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "^1.1.5",
    "highcharts-series-generator": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.20.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
