{
  "name": "@ebi-gene-expression-group/scxa-cell-type-wheel",
  "version": "1.1.0-beta1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Cell type wheel chart",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Haider Iqbal <haider@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlas-components.git"
  },
  "dependencies": {
    "highcharts": "^9.1.0",
    "highcharts-react-official": "^3.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0",
    "urijs": "^1.19.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@ebi-gene-expression-group/eslint-config": "^2.4.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@webpack-cli/serve": "^1.4.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.12.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.27.0",
    "fetch-mock": "^9.11.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "webpack": "^5.37.1",
    "webpack-dev-server": "^4.0.0-beta.3"
