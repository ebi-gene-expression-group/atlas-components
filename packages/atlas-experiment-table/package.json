{
  "name": "@ebi-gene-expression-group/atlas-experiment-table",
  "version": "3.2.0",
  "description": "This package renders an experiment table with user interaction using Evergreen package",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ],
    "coveragePathIgnorePatterns": [
      "TestUtils.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Lingyun Zhao<lingyun@ebi.ac.uk>",
    "Haider Iqbal<haideri@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-gene-expression-group/atlas-components.git"
  },
  "dependencies": {
    "evergreen-ui": "^5.0.4",
    "jw-paginate": "^1.0.4",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-popup": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.2.8",
    "styled-components": "^5.1.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "fetch-mock": "^9.10.6",
    "jest": "^26.4.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "random-string": "^0.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "gitHead": "d05d91b908daff790db373725899d05ddc6d2579"
}
