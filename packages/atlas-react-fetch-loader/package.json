{
  "name": "@ebi-gene-expression-group/atlas-react-fetch-loader",
  "version": "3.10.0",
  "description": "A React HOC that enables other components to remotely fetch data from an endpoint",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/requestAnimationFrame.js"
    ]
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-gene-expression-group/atlas-components.git"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.6.0",
    "fetch-mock": "^9.11.0",
    "jest": "^29.7.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^4.2.0",
    "node-fetch": "^2.6.0",
    "react-test-renderer": "^16.13.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/ebi-gene-expression-group/atlas-react-fetch-loader/issues"
  },
  "homepage": "https://github.com/ebi-gene-expression-group/atlas-react-fetch-loader#readme",
