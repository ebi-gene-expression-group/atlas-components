{
  "name": "@ebi-gene-expression-group/react-ebi-species",
  "version": "2.4.1",
  "description": "React component based on the EBI species icons",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "contributors": [
    "Lingyun Zhao<lingyun@ebi.ac.uk>",
    "Haider Iqbal<haideri@ebi.ac.uk>",
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
    "Wojciech Bazant <wbazant@ebi.ac.uk>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-gene-expression-group/atlas-components.git"
  },
  "keywords": [
    "react",
    "icon",
    "ebi",
    "species",
    "ensembl",
    "bioinformatics",
    "biology",
    "rabbit",
    "chicken"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@ebi-gene-expression-group/eslint-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "bugs": {
    "url": "https://github.com/ebi-gene-expression-group/react-ebi-species/issues"
  },
  "homepage": "https://github.com/ebi-gene-expression-group/react-ebi-species#readme",
  "directories": {
    "lib": "lib"
  }
}
